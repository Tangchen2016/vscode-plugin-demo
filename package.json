{
	"name": "helloworld",
	"displayName": "HelloWorld",
	"description": "hello world",
	"version": "0.0.1",
	"publisher": "vscode-sample",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:javascript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"type": "object",
			"title": "跳转插件",
			"properties": {
				"vscodePluginAlias.basePath": {
						"type": "string",
						"default": "/Users/tangchen/workspace/code/gitRepo/report-web",
						"description": "相对路径"
				},
				"vscodePluginAlias.config": {
					"type": "object",
					"markdownDescription": "alias规则配置地址（相对项目路径）\n\rExample: \n\r```\r\"vscodePluginAlias.path\": {\r\t\"@\": \"src\"\r}\r```"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/moment": "^2.13.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"moment": "^2.24.0"
	}
}
